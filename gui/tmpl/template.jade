    template.dummy-report
      .pane__titlebar(data-bind="text:title")
      .pane__body
        .pane__page
          h1(data-bind="text:subhead")
          div(data-bind="element:content")

    template.dummy-file
      .pane__titlebar(data-bind="text:title")
      .pane__body
        .pane__page
          h1(data-bind="text:subhead")
